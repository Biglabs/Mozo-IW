<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity WalletAddress.
    -->
    <changeSet id="20180621103733-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="wallet_address"
                                 constraintName="fk_wallet_address_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"/>

        <addForeignKeyConstraint baseColumnNames="wallet_id"
                                 baseTableName="wallet_address"
                                 constraintName="fk_wallet_address_wallet_id"
                                 referencedColumnNames="id"
                                 referencedTableName="wallet"/>

    </changeSet>
</databaseChangeLog>
