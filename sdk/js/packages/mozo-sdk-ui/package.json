{
  "name": "mozo-sdk-ui",
  "version": "0.0.12",
  "description": "Mozo wallet component",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.0.8",
    "custom-protocol-detection": "^1.0.1",
    "qrcode": "^1.2.2"
  },
  "devDependencies": {
    "@stencil/core": "^0.11.0",
    "@stencil/sass": "^0.1.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Biglabs/Mozo-IW.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Biglabs/Mozo-IW/tree/master/sdk/js"
  },
  "homepage": "https://github.com/Biglabs/Mozo-IW/tree/master/sdk/js",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
