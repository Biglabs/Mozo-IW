{
  "presets": [
    "@babel/preset-env",
    "@babel/react",
	"module:metro-react-native-babel-preset"
  ],
  "plugins": [
    ["react-native-web", { commonjs: true }],
	["@babel/plugin-proposal-decorators", { "legacy": true }],
	["@babel/plugin-transform-runtime", {
      "corejs": false,
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }]
  ]
}
