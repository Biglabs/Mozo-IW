{
  "name": "MozoWallet",
  "version": "0.0.1",
  "private": true,
  "main": "./desktop/main.js",
  "homepage": "https://github.com/Biglabs/Mozo-IW",
  "scripts": {
    "reset-cache": "react-native start --config=rn-cli.config.js --reset-cache",
    "start": "node node_modules/react-native/local-cli/cli.js start --config=rn-cli.config.js --resetCache",
    "postinstall": "npm rebuild --target=2.0.2 --runtime=electron --dist-url=https://atom.io/download/electron grpc",
    "android": "yarn && ./node_modules/.bin/rn-nodeify --hack --install && react-native run-android",
    "android-release": "yarn && ./node_modules/.bin/rn-nodeify --hack --install && react-native run-android --variant=release",
    "ios": "yarn && ./node_modules/.bin/rn-nodeify --hack --install && react-native run-ios",
    "ios-release": "yarn && ./node_modules/.bin/rn-nodeify --hack --install && react-native run-ios --configuration Release",
    "desktop": "webpack && electron ./desktop/main.js",
    "desktop:dev": "webpack-dev-server --hot --inline",
    "webpack": "webpack --progress",
    "webpack-server": "webpack-serve",
    "pack": "build --dir",
    "dist": "build",
    "build:win": "cd desktop && yarn && cd .. && build --win --dir",
    "build:mac": "cd desktop && yarn && cd .. && build --mac --dir",
    "build:linux": "cd desktop && yarn && cd .. && build --linux --dir",
    "dist:win": "build --win",
    "dist:linux": "build --linux",
    "dist:mac": "build --mac",
    "zip": "cross-zip ./dist/SOLOSigner-0.0.1.dmg"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "assert": "^1.1.1",
    "asyncstorage-down": "^4.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-regenerator-runtime": "6.5.0",
    "bip38": "^2.0.2",
    "bip39": "^2.5.0",
    "bitcoinjs-lib": "^3.3.2",
    "browserify-zlib": "~0.1.4",
    "buffer": "^4.9.1",
    "console-browserify": "^1.1.0",
    "constants-browserify": "^1.0.0",
    "crypto-js": "^3.1.9-1",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.1.1",
    "electron-settings": "^3.2.0",
    "es6-symbol": "^3.1.1",
    "ethereumjs-tx": "^1.3.4",
    "ethereumjs-wallet-react-native": "^0.6.7",
    "events": "^1.0.0",
    "global": "^4.3.2",
    "grpc": "^1.14.1",
    "https-browserify": "~0.0.0",
    "inherits": "^2.0.1",
    "js-sha512": "^0.7.1",
    "keycloak-js": "^4.4.0",
    "mobx": "3.6.2",
    "mobx-react": "4.4.3",
    "path-browserify": "0.0.0",
    "process": "^0.11.0",
    "prop-types": "^15.6.2",
    "punycode": "^1.4.1",
    "qrcode": "^1.2.2",
    "querystring-es3": "~0.2.0",
    "ramda": "^0.25.0",
    "raw-loader": "^0.5.1",
    "react": "16.3.1",
    "react-art": "^16.3.1",
    "react-dom": "16.3.1",
    "react-native": "^0.55.4",
    "react-native-bitcoinjs-lib": "^2.3.9",
    "react-native-camera": "^1.1.4",
    "react-native-countdown-component": "^1.7.1",
    "react-native-crypto": "^2.1.2",
    "react-native-electron": "0.7.0",
    "react-native-file-selector": "^0.0.3",
    "react-native-fs": "^2.10.14",
    "react-native-http": "tradle/react-native-http#834492d",
    "react-native-i18n": "^2.0.15",
    "react-native-level-fs": "^3.0.0",
    "react-native-os": "^1.0.1",
    "react-native-qrcode-svg": "^5.0.7",
    "react-native-randombytes": "^3.2.0",
    "react-native-router-flux": "4.0.0-beta.31",
    "react-native-safe-crypto": "^1.0.1",
    "react-native-secure-randombytes": "^2.2.3",
    "react-native-share": "^1.0.27",
    "react-native-svg": "^6.4.1",
    "react-native-svg-uri": "^1.2.3",
    "react-native-tcp": "^3.3.0",
    "react-native-udp": "^2.3.1",
    "react-native-web": "0.8.8",
    "react-qr-reader": "^2.1.0",
    "react-svg-inline": "^2.1.1",
    "react-timer-mixin": "^0.13.3",
    "readable-stream": "1.0.33",
    "realm": "2.13.0",
    "request": "^2.88.0",
    "save": "^2.3.2",
    "simple-jsonrpc-js": "^0.0.10",
    "simple-oauth2": "^2.2.1",
    "stream-browserify": "^1.0.0",
    "string_decoder": "~0.10.25",
    "svgs": "^3.2.1",
    "timers-browserify": "^1.0.1",
    "tty-browserify": "0.0.0",
    "url": "~0.10.1",
    "util": "~0.10.3",
    "vm-browserify": "0.0.4",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-react-native-web": "^0.8.8",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-mobx": "^1.0.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-zip-cli": "^1.0.0",
    "electron": "2.0.2",
    "electron-builder": "20.24.4",
    "eslint": "^5.2.0",
    "jest": "23.1.0",
    "metro": "^0.43.5",
    "metro-core": "^0.43.5",
    "react-test-renderer": "16.3.1",
    "rn-nodeify": "mvayngrib/rn-nodeify",
    "url-loader": "^1.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "3.0.3",
    "webpack-dev-server": "^3.1.9"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browser": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "build": {
    "appId": "signer.biglabs.com",
    "asar": true,
    "compression": "store",
    "files": [
      "desktop/",
      "!node_modules/",
      {
        "from": "desktop/node_modules",
        "to": "node_modules"
      }
    ],
    "win": {
      "target": [
        {
          "target" : "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./assets/icon/mozo_offchain.ico"
    },
    "nsis" : {
      "artifactName" : "${productName}_${version}_${arch}.${ext}",
      "oneClick" : false,
      "perMachine" : false,
      "license" : "./assets/installation/EULA.txt"
    },
    "mac": {
      "category": "signer.biglabs.com.crypto"
    },
    "linux": {
      "target": "deb",
      "category": "Internet"
    },
    "protocols": {
      "name": "solosigner",
      "schemes": [
        "solosigner"
      ]
    }
  },
  "author": {
    "name": "Biglabs",
    "email" : "developer@mozocoin.io"
  }
}
